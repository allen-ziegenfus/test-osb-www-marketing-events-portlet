{
	"$event = /osb.marketingevent/get-marketing-events": {
		"type": 1,
		"start": -1,
		"end": -1,
		"$sessions = /osb.marketingeventsession/get-marketing-event-sessions": {
			"@marketingEventId": "$event.marketingEventId",
			"start": -1,
			"end": -1
		},
		"$sponsors = /osb.marketingeventsponsor/get-marketing-event-sponsors": {
			"@marketingEventId": "$event.marketingEventId",
			"start": -1,
			"end": -1
		},
		"$sponsorlevels = /osb.marketingeventsponsorlevel/get-marketing-event-sponsor-levels": {
			"@marketingEventId": "$event.marketingEventId",
			"start": -1,
			"end": -1
		},
		"$marketingEventUsers =  /osb.marketingeventuser/get-marketing-event-users": {
			"@marketingEventId": "$event.marketingEventId",
			"start": -1,
			"end": -1
		}
	}
}